{
  "name": "ethcreatures",
  "type": "module",
  "peerDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "p-map-series": "^3.0.0",
    "svgo": "^3.0.2",
    "typescript": "^5.0.3",
    "vite": "^4.4.2"
  },
  "scripts": {
